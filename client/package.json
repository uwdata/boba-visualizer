{
  "name": "boba-visualizer",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "description": "Visual analysis of multiverse outcomes",
  "author": {
    "name": "Yang Liu",
    "url": "http://yangliu.life"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yyyliu/boba-visualizer"
  },
  "scripts": {
    "build": "npm run prebuild && npm run webpack-prod",
    "dev": "npm run prebuild && npm run webpack-dev",
    "prebuild": "rm -rf dist && mkdir dist && cp -R assets/ dist && cp index.html dist",
    "lint": "eslint --ext .js,.vue src",
    "webpack-dev": "webpack --config webpack.dev.js",
    "webpack-prod": "webpack --progress --hide-modules --config webpack.prod.js",
    "start": "cd dist && python -m SimpleHTTPServer 8080"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  }
}
